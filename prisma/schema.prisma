generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model servers {
  id         Int    @id @default(autoincrement())
  guild      String @db.VarChar(24)
  name       String @db.VarChar(30)
  ip         String @db.VarChar(255)
  icon_cache String?
  icon_hash  String?
}
